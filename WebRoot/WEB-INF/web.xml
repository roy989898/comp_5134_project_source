<?xml version="1.0" encoding="UTF-8"?>  
<web-app version="2.5"   
    xmlns="http://java.sun.com/xml/ns/javaee"   
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"   
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee   
    http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"> 
  <display-name></display-name>
  
  <servlet><!--servlet类路径配置-->  
    <servlet-name>Login</servlet-name>  
    <servlet-class>cn.com.servlet.Login</servlet-class>  
  </servlet> 
  <servlet><!--servlet类路径配置-->  
    <servlet-name>ajaxSubmitCourse</servlet-name>  
    <servlet-class>cn.com.servlet.AjaxSubmitCourse</servlet-class>  
  </servlet>    
    
  <servlet-mapping><!--servlet类映射配置-->  
    <servlet-name>Login</servlet-name>  
    <url-pattern>/Login</url-pattern>  
  </servlet-mapping>  
  <servlet-mapping><!--servlet类映射配置-->  
    <servlet-name>ajaxSubmitCourse</servlet-name>  
    <url-pattern>/ajaxSubmitCourse</url-pattern>  
  </servlet-mapping>  
  
  
  
  <welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
  </welcome-file-list>
</web-app>